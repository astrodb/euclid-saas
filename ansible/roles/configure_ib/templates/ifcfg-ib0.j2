# {{ ansible_managed }}
#
BOOTPROTO=none
TYPE=InfiniBand
DEVICE=ib0
ONBOOT=yes
USERCTL=no
IPADDR={{ ib_ipaddr }}
NETMASK={{ ib_netmask }}
{% if ib_hwaddr != None %}
HWADDR={{ ib_hwaddr }}
{% endif %}
{% if ib_mtu != None %}
MTU={{ ib_mtu }}
{% endif %}
{% if ib_connected_mode != None %}
CONNECTED_MODE={{ ib_connected_mode }}
{% endif %}
